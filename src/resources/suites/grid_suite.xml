<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >

<suite name="Parallel test" verbose="1" parallel="tests" thread-count="5">

    <test name="Google test Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="academy.softserve.edu.tests.GridTestParallel">
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>

    <test name="Google test Chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="academy.softserve.edu.tests.GridTestParallel">
                <methods>
                    <include name="test"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>