{
  "capabilities": [
    {
      "platform": "WINDOWS",
      "browserName": "firefox",
      "version": "46",
      "maxInstances": 3,
      "seleniumProtocol": "WebDriver"
    },
    {
      "platform": "WINDOWS",
      "browserName": "chrome",
      "version": "43",
      "maxInstances": 3,
      "seleniumProtocol": "WebDriver",
      "webdriver.chrome.driver": "chromedriver.exe"
    },
    {
      "platform": "WINDOWS",
      "browserName": "internet explorer",
      "version": "8",
      "maxInstances": 3,
      "seleniumProtocol": "WebDriver",
      "webdriver.ie.driver": "IEDriverServer.exe"
    }
  ],
  "configuration": {
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "maxSession": 5,
    "port": 5555,
    "host": "localhost",
    "register": true,
    "registerCycle": 5000,
    "hubPort": 4444,
    "hubHost": "localhost"
  }
}